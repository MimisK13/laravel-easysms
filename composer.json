{
    "name": "mimisk13/laravel-easysms",
    "description": "A Laravel package for integrating EasySMS.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mimis K",
            "email": "mimisk88@gmail.com",
            "homepage": "https://mimisk.com"
        }
    ],
    "homepage": "https://github.com/mimisk13/laravel-easysms",
    "keywords": ["Laravel", "EasySMS"],
    "require": {
        "guzzlehttp/guzzle": "^7.0|^8.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mimisk13\\LaravelEasySMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mimisk13\\LaravelEasySMS\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mimisk13\\LaravelEasySMS\\EasySMSServiceProvider"
            ],
            "aliases": {
                "EasySMS" : "Mimisk13\\LaravelEasySMS\\Facades\\EasySMS"
            }
        }
    }
}
